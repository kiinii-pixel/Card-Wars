config_version=5

[application]

config/name="Card Wars"
config/description="Adventure Time: Card Wars made in Godot engine.
Card Wars is a casual deckbuilding card game in the land of Ooo."
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://assets/images/other/icon.png"

[autoload]

Global="*res://scripts/global.gd"
StaticData="res://data/static_data.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

